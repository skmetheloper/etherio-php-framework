{
  "name": "etherio/ethereal",
  "description": "The PHP Development Framework",
  "version": "1.0.0",
  "license": "MIT",
  "type": "project",
  "keywords": ["ethereal", "etherio", "framework"],
  "require": {
    "php": "^7.2.1",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Ethereal\\": "src/Ethereal/"
    },
    "classmap": [
      "database/seeds"
    ],
    "files": [
      "src/Ethereal/Foundation/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
    ],
    "post-autoload-dump": [
    ]
  }
}
